{
  "name": "eleventy-tailwind-starter",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npx @11ty/eleventy --serve\" \"npm:css:watch\"",
    "css:build": "tailwindcss -i ./src/styles/tailwind.css -o ./public/assets/main.css --minify",
    "css:watch": "tailwindcss -i ./src/styles/tailwind.css -o ./public/assets/main.css --watch",
    "build": "npm run css:build && npx @11ty/eleventy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "luxon": "^3.7.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  }
}
